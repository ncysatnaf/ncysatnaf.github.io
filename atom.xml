pan&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;    onPress=&amp;#123;&lt;span class=&quot;keyword&quot;&gt;this&lt;/span&gt;.onPress.bind(&lt;span class=&quot;keyword&quot;&gt;this&lt;/span&gt;)&amp;#125;  &lt;span class=&quot;comment&quot;&gt;//not work&lt;/span&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;    onPress=&amp;#123;() =&amp;gt; &amp;#123;&lt;span class=&quot;keyword&quot;&gt;this&lt;/span&gt;.onPress&amp;#125;.bind(&lt;span class=&quot;keyword&quot;&gt;this&lt;/span&gt;)&amp;#125; &lt;span class=&quot;comment&quot;&gt;//undefined is not an object&lt;/span&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;&lt;span class=&quot;comment&quot;&gt;//&amp;gt;&lt;/span&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;	 &lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;&lt;span class=&quot;comment&quot;&gt;//解决办法&lt;/span&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;&lt;span class=&quot;keyword&quot;&gt;let&lt;/span&gt; onPress = &lt;span class=&quot;keyword&quot;&gt;this&lt;/span&gt;.onPress&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;&lt;span class=&quot;comment&quot;&gt;//render()....&lt;/span&gt;&lt;/span&gt;&lt;br&gt;&lt;span class=&quot;line&quot;&gt;onPress = &amp;#123;onPress&amp;#125;&lt;/span&gt;&lt;br&gt;&lt;/pre&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/table&gt;&lt;/figure&gt;
